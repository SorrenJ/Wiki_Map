  <link rel="stylesheet" href="/styles/main.css" />
  <link rel="stylesheet" href="/styles/layout.css" />

  <script src="/vendor/jquery-3.0.0.js"></script>
  <script defer src="/scripts/app.js"></script>
</head>

<body>
  <h1>Create new map</h1>
  <form action="/" method="POST">
    <label for="map_title">Map title</label>
    <textarea name="text" id="map_title"></textarea>

    <label for="location_title">Location title</label>
    <textarea name="text" id="location_title"></textarea>


    <label for="upload_image">Upload image</label>
    <textarea name="text" id="upload_image"></textarea>


    <button type="submit" id="newMap">Create</button>


  </form>
  <div id="map" style="height: 50vh"></div>

</body>

</html>
